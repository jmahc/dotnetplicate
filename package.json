{
  "name": "dotnetplicate",
  "version": "0.1.0",
  "description": "Replicate files in your dotnetcore project from the command-line.",
  "repository": "https://github.com/jmahc/dotnetplicate/",
  "author": {
    "name": "Jordan McArdle",
    "email": "jordan@mcardle.tech",
    "url": "http://mcardle.tech/"
  },
  "engines": {
    "node": ">=6.0.0",
    "yarn": ">=0.27.5"
  },
  "bin": {
    "generact": "dist/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "babel src -d dist",
    "clean": "rimraf dist",
    "clean-dev": "rimraf node_modules yarn.lock .yarnclean .DS_STORE",
    "flow": "flow check",
    "lint": "eslint src",
    "postmerge": "yarn",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "release": "standard-version"
  },
  "watch": {
    "build": "src",
    "lint": "{src,test}/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "^0.51.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "npm-watch": "^0.2.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.5.3",
    "prettier-eslint": "^6.4.2",
    "prettier-standard": "^6.0.0",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "suppose": "^0.6.2"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "inquirer": "^3.2.1",
    "inquirer-autocomplete-prompt": "^0.11.0",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "ora": "^1.3.0"
  },
  "homepage": "https://github.com/jmahc/dotnetplicate#readme",
  "bugs": {
    "url": "https://github.com/jmahc/dotnetplicate/issues"
  },
  "license": "MIT",
  "keywords": [
    "cli",
    "dotnet",
    "dotnetcore",
    "dotnetplicate",
    "generator",
    "replicate"
  ]
}
